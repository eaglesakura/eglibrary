<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SupportRecyclerView">
        <attr name="emptyText" format="string"/>
    </declare-styleable>

    <declare-styleable name="MaterialButton">
        <attr name="buttonBaseColor" format="color"/>
        <attr name="buttonHighlightWeight" format="float"/>
        <attr name="buttonTextColorMode" format="enum">
            <enum name="light" value="0"/>
            <enum name="dark" value="1"/>
            <enum name="palette" value="2"/>
            <enum name="auto" value="3"/>
        </attr>
    </declare-styleable>
</resources>